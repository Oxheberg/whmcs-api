{
  "name": "@oxheberg/whmcs-api",
  "version": "0.0.0",
  "description": "A simple TypeScript wrapper for the WHMCS API",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.cjs"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "dev": "tsup src/index.ts --watch",
    "prepare": "husky",
    "format": "biome format",
    "lint": "biome lint",
    "check": "biome check --staged --write"
  },
  "keywords": [
    "whmcs",
    "api",
    "wrapper",
    "typescript"
  ],
  "author": "Oxheberg",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/oxheberg/whmcs-api.git"
  },
  "homepage": "https://github.com/oxheberg/whmcs-api#readme",
  "bugs": {
    "url": "https://github.com/oxheberg/whmcs-api/issues"
  },
  "contributors": [
    {
      "name": "Zerga Abderrezak",
      "email": "zerga@oxheberg.com",
      "url": "https://github.com/zergaabderrezak"
    }
  ],
  "packageManager": "pnpm@10.12.4",
  "devDependencies": {
    "@biomejs/biome": "2.0.6",
    "husky": "^9.1.7",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  }
}
